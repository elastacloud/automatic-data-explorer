Package: AutoExploreR
Type: Package
Title: Automatic Data Exploration
Version: 0.1.0
Date: 2017-07-17
Authors@R: c(
    person("Andrew", "Booth", email = "andrew@elastacloud.com", role = "aut"),
    person("Darshna", "Shah", email = "darshna@elastacloud.com", role = "aut"),
